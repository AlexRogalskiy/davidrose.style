extends layout

block vars
  - var pageTitle = 'Dress David Rose'
  - var pageDescription = 'Every shirt, sweater, and jacket David Rose wears on Schitt’s Creek.'
  - var bodyClass = 'home'

mixin gallery-svg(season,episode,item,alt)
  .gallery-shirt(class='season-0' + season)
    a(href='#s0' + season + 'e' + episode + '-' + item)
      img(src='/assets/images/gallery/s0' + season + 'e' + episode + '-' + item + '.svg', alt=alt, loading='lazy')
      figcaption='S0' + season + ' E' + episode

mixin gallery-png(season,episode,item,alt)
  .gallery-shirt(class='season-0' + season)
    a(href='#s0' + season + 'e' + episode + '-' + item)
      picture
        source(srcset='/assets/images/gallery/s0' + season + 'e' + episode + '-' + item + '-sm.png', media='(max-width: 845px) and (max-height: 800px)')
        source(srcset='/assets/images/gallery/s0' + season + 'e' + episode + '-' + item + '.png')
        img(src='/assets/images/gallery/s0' + season + 'e' + episode + '-' + item + '.png', alt=alt, loading='lazy')
      figcaption='S0' + season + ' E' + episode

mixin feature-svg(season,episode,item,prev,next,alt)
  .shirt(id='s0' + season + 'e' + episode + '-' + item)
    a.prev(href='#' + prev) Prev
    img(src='/assets/images/feature/s0' + season + 'e' + episode + '-' + item + '.svg', alt=alt)
    figcaption='S0' + season + ' E' + episode
    a.next(href='#' + next) Next

mixin feature-png(season,episode,item,prev,next,alt)
  .shirt(id='s0' + season + 'e' + episode + '-' + item)
    a.prev(href='#' + prev) Prev
    picture
      source(srcset='/assets/images/feature/s0' + season + 'e' + episode + '-' + item + '-sm.png', media='(max-width: 845px) and (max-height: 630px)')
      source(srcset='/assets/images/feature/s0' + season + 'e' + episode + '-' + item + '-md.png', media='(max-width: 845px) and (min-height: 631px)')
      source(srcset='/assets/images/feature/s0' + season + 'e' + episode + '-' + item + '.png')
      img(src='/assets/images/feature/s0' + season + 'e' + episode + '-' + item + '.png', alt=alt)
    figcaption='S0' + season + ' E' + episode
    a.next(href='#' + next) Next

